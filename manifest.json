{
    "name": "Productive Time",
    "version": "1.0",
    "description": "Productivity extension",
    "permissions": ["storage"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html"
      },
    "icons": {
        "16": "images/16.png",
        "32": "images/32.png",
        "48": "images/48.png",
        "128": "images/128.png"
    },
    "options_page": "options.html",
    "author": "Alexis Morel",
    "manifest_version": 2
}